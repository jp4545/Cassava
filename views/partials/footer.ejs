<script type="text/javascript">
  $(document).ready(function(){
         $("#contact_us").click(function(e){

          e.preventDefault();
          var email = $("#input-email").val();
          var name = $("#input-name").val();
          var subject = $("#input-subject").val();
          var enquiry = $("#input-enquiry").val();
          $.ajax({
             type: "POST",
                      url: "/contact",
                      data:{name:name,email:email,subject:subject,enquiry:enquiry},
                    success: function(success) {
                          if(success=="inserted")
                          {
                            alert("Your request has been sent, Admin will contact you soon");
                            location.reload();
                          }
                          if(success=="not_inserted")
                          {
                            alert("An Eror has occured, Please resent your request");
                            location.reload();
                          }
                          
          },
                    error: function(jqXHR, textStatus, err) {
                       alert('Oops! Something has gone wrong. Check your Connection');
                        location.reload();

                       
                      }
                  
                  });
          });
   });
</script>
<script type="text/javascript">
function validateForm() {
  var name = document.forms["myForm"]["name"].value;
  var email = document.forms["myForm"]["email"].value;
  var subject = document.forms["myForm"]["subject"].value;
  if (name == "") {
    alert("Name must be filled out");
    return false;
  }
  if (email == "") {
    alert("Email must be filled out");
    return false;
  }
  if (subject == "") {
    alert("subject must be filled out");
    return false;
  }
}
